{
  "name": "@backtestjs/core",
  "version": "1.1.0",
  "description": "Free and easy way to write backtest trading strategies in TS / JS with built in candle data retrieval",
  "main": "dist/main",
  "typings": "dist/main",
  "keywords": [
    "backtestjs",
    "backtest",
    "finance",
    "trading",
    "download historical candle data",
    "indicators",
    "multi value",
    "multi symbol"
  ],
  "scripts": {
    "prebuild": "rm -rf dist esm",
    "build:esm": "tsc --target es2018 --outDir esm",
    "build:cjs": "tsc --target es2015 --module commonjs --outDir dist",
    "build": "npm run build:esm && npm run build:cjs",
    "prestart": "npm run build",
    "start": "node --env-file=.env dist/main.js",
    "predemo-js": "npm run build",
    "demo-js": "node --env-file=.env dist/src/demo.js",
    "demo": "npx ts-node src/demo.ts",
    "dev": "npx ts-node main.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "npm run build"
  },
  "files": [
    "dist",
    "esm",
    "prisma/schema.prisma"
  ],
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ]
  },
  "author": "Andrew Baronick",
  "license": "Apache-2.0",
  "dependencies": {
    "@prisma/client": "^5.21.0",
    "axios": "^1.4.0",
    "csvtojson": "^2.0.10",
    "technicalindicators": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "prisma": "^5.21.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
